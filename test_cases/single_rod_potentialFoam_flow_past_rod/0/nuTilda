FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    top
    {
    	type            zeroGradient;
    }

    bottom
    {
        type        zeroGradient;
    }

    left
    {
     	type            zeroGradient;
    }

    right
    {
       type            fixedValue;
       value       	uniform 0;
    }

    front
    {
         type            zeroGradient;
    }

    back
    {
        type            zeroGradient;
    }

    cylinder
    {
       type            zeroGradient;
    }
}

