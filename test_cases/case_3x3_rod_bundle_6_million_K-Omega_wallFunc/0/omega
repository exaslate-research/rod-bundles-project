FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform  0.12575;	//14.855;

boundaryField
{

    top
    {
    	//type            pressureInletOutletVelocity;
        //value           $internalField;
    	//type        fixedValue;
        //value       $internalField;//uniform (0 0 0);
        type        	zeroGradient;
        /*type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;  */            
    }

    bottom
    {
        type        	fixedValue;
        value      	$internalField;	
    }

    left
    {
        type            omegaWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           $internalField;	
    }

    right
    {
        type            omegaWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           $internalField;	
        // type             noSlip;
        //value           uniform (0 0 0);
        
    }

    front
    {
        type            omegaWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           $internalField;	
        //type         	noSlip;
       // value       uniform (0 0 0);
    }

    back
    {
        type            omegaWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           $internalField;	
    }

    cylinder
    {
        type            omegaWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           $internalField;	
    }
}
