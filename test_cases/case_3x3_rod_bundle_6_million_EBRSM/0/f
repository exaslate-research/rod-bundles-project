FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      f;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 0 0 0 0 ];

internalField   uniform 1;

boundaryField
{

    top
    {
        type        fixedValue;	//cyclic;	//fixedValue;
        value       uniform 0;		//$internalField;	//uniform 0;
    }

    bottom
    {
       type        fixedValue;	//cyclic;	//fixedValue;
        value       uniform 0;
    }

    left
    {
        type        fixedValue;		//kqRWallFunction;
        value       uniform 0;		//0.00375;
    }

    right
    {
        type        fixedValue;		//kqRWallFunction;
        value       uniform 0;		//0.00375;
    }

    front
    {
        type        fixedValue;		//kqRWallFunction;
        value       uniform 0;		//0.00375;
    }

    back
    {
        type        fixedValue;		//kqRWallFunction;
        value       uniform 0;		//0.00375;
    }

    cylinder
    {
        type        fixedValue;		//kqRWallFunction;
        value       uniform 0;		//0.00375;
    }

