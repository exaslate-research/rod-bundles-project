    }

    left
    {
        type            zeroGradient;
    }

    right
    {
        type        zeroGradient;
    }

    front
    {
        type            zeroGradient;
    }

    back
    {
        type            zeroGradient;
    }

    cylinder
    {
        type            zeroGradient;
    }
