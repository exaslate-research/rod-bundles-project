FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.00375;

boundaryField
{

    top
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;           
    }

    bottom
    {
        type        fixedValue;
        value       uniform 0.00375;
    }

    left
    {
     	type            kqRWallFunction;
        value           uniform 0.00375;
      //type            noSlip;
    }

    right
    {
         type            kqRWallFunction;
        value           uniform 0.00375;        
    }

    front
    {
         type            kqRWallFunction;
        value           uniform 0.00375;
    }

    back
    {
         type            kqRWallFunction;
        value           uniform 0.00375;
        //type         noSlip;
        //value       uniform (0 0 0);
    }

    cylinder
    {
        type            kqRWallFunction;
        value           uniform 0.00375;
    }
}


