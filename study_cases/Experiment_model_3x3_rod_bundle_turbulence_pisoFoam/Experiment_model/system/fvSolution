FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "alpha.water.*"
    {
        nAlphaCorr      2;
        nAlphaSubCycles 1;
        cAlpha          1;

        MULESCorr       yes;
        nLimiterIter    5;
	solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-8;
        relTol          0;
    }

    "pcorr.*"
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-5;
        relTol          0;
    }

    //p_rgh
    p
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-07;
        relTol          0.05;
    }

    //p_rghFinal
    pFinal
    {
//        $p_rgh;
        $p;
        relTol          0;
    }

    U
    {
        solver          /*PBiCGStab;*/smoothSolver;
        //preconditioner  DILU;
        smoother        symGaussSeidel;
        tolerance       1e-06;
        relTol          0;
    }
}
SIMPLE
{
    nNonOrthogonalCorrectors 0;

    residualControl
    {
        p              1e-4;// 1e-2;
        U               1e-6;
        "(k|epsilon)" 1e-4;
    }
}
relaxationFactors
{
    fields
    {
        p               0.3;
    }
    equations
    {
        U               0.7;
        k               0.7;
        epsilon         0.7;
    }
}

