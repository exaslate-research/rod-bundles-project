FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pisoFoam;//interFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         500;		//0.05;

deltaT          1;		//0.0001;

writeControl    timeStep;	//adjustableRunTime;//timestep;

writeInterval   11;		//0.001;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;//compressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

